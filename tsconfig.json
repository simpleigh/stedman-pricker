{
    "compilerOptions": {
        "declaration": true,
        "newLine": "LF",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "strictNullChecks": true
    },
    "include": [
        "src/**/*.ts"
    ]
}

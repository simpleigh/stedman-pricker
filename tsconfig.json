{
    "compileOnSave": false,
    "compilerOptions": {
        "alwaysStrict": true,
        "declaration": true,
        "newLine": "LF",
        "noEmitOnError": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "outFile": "dist/stedman-pricker.js",

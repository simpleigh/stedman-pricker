/**
 * Renders a course as HTML for MBD-style pricker
 */
<u>{{= data.initialRow }}</u><br />
{{~ data.sixes :six }}
	{{= six.end }}
	&nbsp;&nbsp;
	<span
	 class="{{= six.index % 2 ? 'odd' : 'even' }}Call"
	 onclick="c({{= six.index }})"
	>
		&nbsp;
		{{? six.call === Pricker.Call.Plain }}
			&nbsp;
		{{?? six.call === Pricker.Call.Bob }}
			-
		{{?? six.call === Pricker.Call.Single }}
			s
		{{?}}
		&nbsp;
	</span>
	&nbsp;&nbsp;
	{{= six.index }}
	<br />
{{~}}

/**
 * Renders a course, e.g.:
 * 480735692E1  s2 3  (4 sixes)\n
 * 2314567890E  p\n
 */

{{= data.courseEnd }}
 /* single space */

{{~ data.sixes :six }}
	{{? six.call }}
		 /* single space */
		{{? six.call === Pricker.Call.Single }}s{{?}}
		{{= six.index }}
	{{?}}
{{~}}

{{? data.isPlainCourse }}
	 /* single space */
	p
{{?}}

{{? data.sixes.length !== data.courseEnd.length * 2 }}
	 /* single space */
	 /* single space */
	({{= data.sixes.length }} sixes)
{{?}}

{{= '\n' }}

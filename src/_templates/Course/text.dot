/**
 * Renders a course, e.g.:
 * 480735692E1  s2 3  (4 sixes)\n
 * 2314567890E  p\n
 */

{{= data.endRow }}
 /* single space */

{{~ data.blocks :six }}
	{{? six.getCall() }}
		 /* single space */
		{{? six.getCall() === Pricker.Call.Single }}s{{?}}
		{{= six.getOwnership()[1] }}
	{{?}}
{{~}}

{{? data.block.isPlain() }}
	 /* single space */
	p
{{?}}

{{? data.blocks.length !== data.endRow.length * 2 }}
	 /* single space */
	 /* single space */
	({{= data.blocks.length }} sixes)
{{?}}

{{= '\n' }}

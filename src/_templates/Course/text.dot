/**
 * Renders a course, e.g.:
 * 480735692E1  s2 3  (4 sixes)\n
 * 2314567890E  p\n
 */

{{= Pricker.stringFromRow(context.object.getEnd()) }}
 /* single space */

{{~ context.blocks :six }}
	{{? six.getCall() }}
		 /* single space */
		{{? six.getCall() === Pricker.Call.Single }}s{{?}}
		{{= six.getOwnership()[1] }}
	{{?}}
{{~}}

{{? context.object.isPlain() }}
	 /* single space */
	p
{{?}}

{{? context.blocks.length !== context.object.getInitialRow().length * 2 }}
	 /* single space */
	 /* single space */
	({{= context.blocks.length }} sixes)
{{?}}

{{= '\n' }}

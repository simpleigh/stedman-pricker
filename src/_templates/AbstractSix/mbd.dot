/**
 * Renders a six as HTML for MBD-style pricker
 * @param {TouchIndex}  musicIndex  - index to check sixes against for music
 * @param {number}      courseIndex - index number of the course
 * @param {string}      type        - six type, either "quick" or "slow"
 */

{{?
	context.musicIndex && context.courseIndex && context.musicIndex.contains(
		context.courseIndex,
		context.object.getOwnership()[1]
	)
}}
	<span class="musicalBlock">
	{{= Pricker.stringFromRow(context.object.getEnd()) }}
	</span>
{{??}}
	{{= Pricker.stringFromRow(context.object.getEnd()) }}
{{?}}

&nbsp;&nbsp;
<span
 class="{{= context.type }}Six"
 onclick="c({{= context.object.getOwnership()[1] }})"
>
	&nbsp;
	{{? context.object.getCall() === Pricker.Call.Plain }}
		&nbsp;
	{{?? context.object.getCall() === Pricker.Call.Bob }}
		-
	{{?? context.object.getCall() === Pricker.Call.Single }}
		s
	{{?}}
	&nbsp;
</span>
&nbsp;&nbsp;
{{= context.object.getOwnership()[1] }}
<br />

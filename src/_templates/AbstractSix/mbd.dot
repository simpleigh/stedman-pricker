/**
 * Renders a six as HTML for MBD-style pricker
 */
{{= context.endRow }}
&nbsp;&nbsp;
<span
 class="{{= context.index % 2 ? 'odd' : 'even' }}Call"
 onclick="c({{= context.index }})"
>
	&nbsp;
	{{? context.block.getCall() === Pricker.Call.Plain }}
		&nbsp;
	{{?? context.block.getCall() === Pricker.Call.Bob }}
		-
	{{?? context.block.getCall() === Pricker.Call.Single }}
		s
	{{?}}
	&nbsp;
</span>
&nbsp;&nbsp;
{{= context.index }}
<br />

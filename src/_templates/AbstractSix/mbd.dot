/**
 * Renders a six as HTML for MBD-style pricker
 */

{{? data.block.getFlag('proof') === false }}
	<span class="false">
{{?}}
{{= data.endRow }}
{{? data.block.getFlag('proof') === false }}
	</span>
{{?}}

&nbsp;&nbsp;

<span
 class="{{= data.index % 2 ? 'odd' : 'even' }}Call"
 onclick="c({{= data.index }})"
>
	&nbsp;
	{{? data.call === Pricker.Call.Plain }}
		&nbsp;
	{{?? data.call === Pricker.Call.Bob }}
		-
	{{?? data.call === Pricker.Call.Single }}
		s
	{{?}}
	&nbsp;
</span>

&nbsp;&nbsp;

{{= data.index }}
<br />

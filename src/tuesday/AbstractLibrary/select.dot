/**
 * Renders a library as HTML for use within a <select> element
 * @param {string}  selected  name of selected item
 */

{{~ context.object.getNames() :name }}
	<option
		{{? name === context.selected }}
		 selected
		{{?}}
		 value="{{= name }}"
	>
		{{= name }}
	</option>
{{~}}

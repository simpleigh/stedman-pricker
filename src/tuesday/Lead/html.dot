/**
 * Renders a lead as HTML for Tuesday night pricker
 * @param {boolean}  underline  whether to underline the sixend
 */

{{? context.underline !== true }}
	{{ context.underline = false; }}
{{?}}

/* Lead head */

{{? context.underline }}
	<u>
{{?}}

	{{= context.stringFromRow(context.object.getLast()) }}

{{? context.underline }}
	</u>
{{?}}

&nbsp;&nbsp;


/* Method */

<select
	 id="method{{= context.object.index }}"
	 onChange="pricker.onMethod({{= context.object.index }})"
>
	{{= context.library.print('select', { selected: context.object.method }) }}
</select>

&nbsp;&nbsp;


/* Index */

{{= context.object.index }}

<br />

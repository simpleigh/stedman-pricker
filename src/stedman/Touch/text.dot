/**
 * Renders a touch as text
 */

{{ var start = context.object.getStart().print('text'); }}

{{= context.stringFromRow(context.object.getStart().getLast()) }}{{= '\n' }}
{{~ context.object.blocks :course }}
	{{= course.print('text', {'end': '\n'}) }}
{{~}}

{{? start }}
	{{= start }}{{= '\n' }}
{{?}}
